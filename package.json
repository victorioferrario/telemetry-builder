{
  "name": "telemetry-builder",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nps",
    "copy": "node ./scripts/copy.js",
    "npm_pack": "cd dist/libs/telemetry-shell && npm pack",
    "build:lib": "ng build telemetry-shell",
    "bump": "bump patch --tag --all  libs/telemetry-shell/package.json",
    "bump:major": "bump major --commit libs/telemetry-shell/package.json",
    "build_all": "npm-run-all bump build:lib npm_pack copy",
    "commit": "git-cz",
    "test": "nps test",
    "update": "ng update @nrwl/schematics"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@nrwl/nx": "6.1.0",
    "core-js": "^2.5.4",
    "fs": "0.0.1-security",
    "fs-extra": "^7.0.1",
    "nps-utils": "^1.7.0",
    "rxjs": "~6.2.0",
    "version-bump-prompt": "^5.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "6.2.8",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nrwl/schematics": "7.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.0",
    "ngrx-store-freeze": "0.2.4",
    "node-notifier": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "nps": "^5.9.5",
    "prettier": "1.13.7",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
